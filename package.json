{
  "name": "houseblock",
  "version": "0.1.0",
  "private": true,
  "description": "HouseBlock - A microservices system for AI, automation, and Web3",
  "scripts": {
    "dev": "pnpm --filter web-landing dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter web-landing start",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "install:all": "pnpm install"
  },
  "devDependencies": {
    "pnpm": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
